{
  "twosplit": {
    "description": "Combined MIDI key split",
    "requires": ["synth@twosplit_k", "synth@twosplit_m"],
    "client": "twosplit",
    "class": "syngrp",
    "in": "twosplit_k:midiin",
    "out1": "twosplit_m:lv2_midi_out_1",
    "out2": "twosplit_m:lv2_midi_out_2",
    "connect": {
      "twosplit_k:midiout": "@twosplit_m.lv2_midi_in"
    }
  }
}
